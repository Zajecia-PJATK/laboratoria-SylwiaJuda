         import java.util.NoSuchElementException;
import java.util.Scanner;

 class Main {
     public static void main(String[] args) {

         int[] numb = {9,99,999};
         int index = 3;

         try {
                 System.out.print("Twoja liczba to: " + numb[index]);

         }
         catch (Exception e) {
             System.out.print("Elementu nie znaleziono.");
         }
     }
 }


